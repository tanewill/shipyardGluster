{
    "credentials": {
        "management": {
            "subscription_id": "$subid",
            "aad": {
                "endpoint": "https://management.core.windows.net/",
                "directory_id": "$dirid",
                "user": "$user",
                "token_cache": {
                    "enabled": true,
                    "filename": ".aad_token_cache"
                }
            }
        },
        "storage": {
            "mystorageaccount": {
                "account": "$saname",
                "account_key": "$sakey",
                "endpoint": "core.windows.net"
            }
        }
    }
}
